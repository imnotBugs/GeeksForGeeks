class Solution {
  public:
    int findExtra(int n, int arr1[], int arr2[]) {
        // add code here.
        int index=-1;
        for(int i=0;i<n;i++){
            if(arr1[i]!=arr2[i]){
                index=i;
                return index;
            }
        }
        if(index== -1)return n;
        
    }
};
